{
	"name": "@maca134/hono-passport",
	"version": "1.1.1",
	"description": "",
	"main": "dist/cjs/index.js",
	"type": "module",
	"module": "dist/index.js",
	"types": "dist/types/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"update-deps": "npm-check-updates -u && npm i",
		"release": "np --no-tests",
		"build": "rimraf dist && tsx ./build.ts && npm run copy:package.cjs.json",
		"copy:package.cjs.json": "shx cp ./package.cjs.json ./dist/cjs/package.json && shx cp ./package.cjs.json ./dist/types/package.json",
		"lint": "eslint --ext js,ts src",
		"lint:fix": "eslint --ext js,ts src --fix",
		"format": "prettier --check \"src/**/*.{js,ts}\"",
		"format:fix": "prettier --write \"src/**/*.{js,ts}\"",
		"postbuild": "publint",
		"prerelease": "npm run build"
	},
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"types": [
				"./dist/types"
			]
		}
	},
	"author": "Matthew McConnell <maca134@googlemail.com> (https://github.com/maca134)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/maca134/hono-passport.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"homepage": "https://github.com/maca134/hono-passport",
	"keywords": [
		"hono"
	],
	"devDependencies": {
		"@hono/node-server": "^1.14.1",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.14.1",
		"@typescript-eslint/eslint-plugin": "^8.31.0",
		"@typescript-eslint/parser": "^8.31.0",
		"esbuild": "^0.25.3",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-define-config": "^2.1.0",
		"eslint-import-resolver-typescript": "^4.3.4",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-node": "^11.1.0",
		"glob": "^11.0.2",
		"np": "^10.2.0",
		"npm-check-updates": "^18.0.1",
		"prettier": "^3.5.3",
		"publint": "^0.3.12",
		"rimraf": "^6.0.1",
		"shx": "^0.4.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"peerDependencies": {
		"@maca134/hono-session": ">=1.*",
		"hono": ">=4.*"
	}
}
